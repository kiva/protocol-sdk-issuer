{
    "headerImage": "kiva_small.jpeg",
    "locale": "en",
    "agent_port": "7567",
    "cloudAgent": "kiva",
    "token": "auth0 token here",
    "verification_options": [
        {
            "id": "QR",
            "title": "Issue Credential to Mobile Wallet",
            "guardian": false,
            "sequence": ["webcam", "registrationForm", "issuance"]
        },
        {
            "id": "QR",
            "title": "Create Cloud Wallet",
            "guardian": false,
            "sequence": ["webcam", "registrationForm", "otp"]
        }
    ],
    "pii_map": {
        "firstName": {
            "name": "First Name",
            "dataType": "text"
        },
        "lastName": {
            "name": "Last Name",
            "dataType": "text"
        },
        "companyEmail": {
            "name": "Company Email",
            "dataType": "text"
        },
        "currentTitle": {
            "name": "Current Title",
            "dataType": "text"
        },
        "team": {
            "name": "Team",
            "dataType": "text"
        },
        "hireDate": {
            "name": "Hire Date",
            "dataType": "date"
        },
        "officeLocation": {
            "name": "Office Location",
            "dataType": "text"
        },
        "phoneNumber": {
            "name": "Phone Number",
            "dataType": "text"
        },
        "photo~attach": {
            "name": "Photo",
            "dataType": "image/jpeg;base64"
        },
        "type": {
            "name": "Type",
            "dataType": "selection",
            "options": ["Full Time", "Intern", "Contractor"]
        },
        "endDate": {
            "name": "End Date",
            "dataType": "date"
        }
    },
    "env": {
        "prod": {
            "ekycURI": "PRODUCTION!",
            "permittedOrigins": "https://www.kiva.org",
            "actions": {
                "includeOnly": ["reset", "enableStandalone"]
            },
            "permittedIframeHosts": ["https://www.kiva.org"],
            "isProd": true
        },
        "sandbox": {
            "ekycURI": "SANDBOX!",
            "inherits": ["prod"]
        },
        "qa": {
            "ekycURI": "https://gateway.protocol-dev.kiva.org/v2/kiva/api/",
            "isProd": true,
            "permittedOrigins": "http://localhost:8080,http://localhost:3000,https://www.kiva.org",
            "permittedIframeHosts": ["https://www.kiva.org", "http://localhost:3000", "http://localhost:8080"]
        },
        "dev": {
            "ekycURI": "DEV!",
            "inherits": ["qa"]
        }
    }
}
