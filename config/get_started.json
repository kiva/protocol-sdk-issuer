{
    "headerImage": "kiva_small.jpeg",
    "locale": "en",
    "agent_port": "7567",
    "cloudAgent": "kiva",
    "verification_options": [
        {
            "id": "QR",
            "title": "Scan QR code",
            "description": "Customer will verify their identity via a QR code on their phone.",
            "guardian": false,
            "sequence": ["webcam", "registrationForm", "issuance"]
        }
    ],
    "pii_map": {
        "nationalId": {
            "name": "National ID",
            "rendered": true,
            "type": "text"
        },
        "firstName": {
            "name": "First Name",
            "rendered": true,
            "type": "text"
        },
        "lastName": {
            "name": "Last Name",
            "rendered": true,
            "type": "text"
        },
        "birthDate": {
            "name": "Birth Date",
            "rendered": true,
            "wide": true,
            "type": "text"
        },
        "faceImage": {
            "name": "Photo",
            "alternateKey": "photoHash",
            "type": "image/jpeg;base64"
        }
    },
    "env": {
        "prod": {
            "ekycURI": "PRODUCTION!",
            "permittedOrigins": "https://www.kiva.org",
            "actions": {
                "includeOnly": ["reset", "enableStandalone"]
            },
            "permittedIframeHosts": ["https://www.kiva.org"],
            "isProd": true
        },
        "sandbox": {
            "ekycURI": "SANDBOX!",
            "inherits": ["prod"]
        },
        "qa": {
            "ekycURI": "QA!",
            "isProd": true,
            "permittedOrigins": "http://localhost:8080,http://localhost:3000,https://www.kiva.org",
            "permittedIframeHosts": ["https://www.kiva.org", "http://localhost:3000", "http://localhost:8080"]
        },
        "dev": {
            "ekycURI": "DEV!",
            "inherits": ["qa"]
        }
    }
}